{
  "User": {
    "id": {
      "int": { "min": 1, "max": 1000000 }
    },
    "email": {
      "regexp": "[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
    },
    "name": {
      "regexp": "[A-Z][a-z]{5,10}"
    },
    "isActive": {
      "boolean": {}
    },
    "lastLogin": {
      "dateTime": {
        "min": "2010-01-01T00:00:00Z",
        "max": "2022-12-31T23:59:59Z"
      }
    },
    "groups": {
      "int": { "min": 1, "max": 1000000 }
    }
  }
}
